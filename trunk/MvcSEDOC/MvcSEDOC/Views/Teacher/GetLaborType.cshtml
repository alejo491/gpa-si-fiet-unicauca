@model IEnumerable<MvcSEDOC.Models.participa>

@{
    ViewBag.Title = "Consultar Calificación";
}

@section MenuSection
{
    @Html.Partial("_MenuTeacherPartial")
}

@section SpecialJavascriptForView
{
    <script src="@Url.Content("~/Scripts/teacher-GetScore.js")" type="text/javascript"></script>    
}

<h2>Consultar Calificación</h2>
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
    <fieldset>
        <h2>@ViewBag.idd</h2>
        <table style="width:100%;">    
            <tr>
                <td>
                    <div> 
                        Tipo de Labor:     @Html.DropDownList("tipoLabor","Labores")
                    </div>
                 </td>                 
            </tr>
            <tr>
                <td>
                    <div class="ui-widget">
                        <div id="work-area" style="min-height: 200px; width: 100%; padding: 5px;" class="ui-widget-content">
                        </div>
                    </div>
                </td>
            </tr>
        </table>       
    </fieldset>
}
